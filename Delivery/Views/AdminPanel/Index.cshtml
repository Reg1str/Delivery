@model IEnumerable<FormOrder>

@{
    
}
<h2>Admin Panel</h2>

<div>
    <ul>
        @foreach (var order in Model)
        {
            <li>
                @order.RecipientsAddress
                -
                <a asp-controller="AdminPanel" asp-action="Edit" asp-route-id="@order.Id">
                    Edit
                </a>
                -
                <a asp-controller="AdminPanel" asp-action="Remove" asp-route-id="@order.Id">
                    Remove
                </a>
            </li>
        }
    </ul>
</div>